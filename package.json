    "svelte-loader": "^3.1.2",
    "webpack": "^5.46.0",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2"
  },
  "dependencies": {
    "rsocket-core": "0.0.27",
    "rsocket-flowable": "0.0.27",
    "rsocket-types": "0.0.27",
    "rsocket-websocket-client": "0.0.27",
    "rsocket-websocket-server": "0.0.27",
    "rxjs": "^7.2.0",
