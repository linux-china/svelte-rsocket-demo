{
  "name": "svelte-rsocket-demo",
  "version": "0.1.0",
  "author": {
    "name": "linux_china"
  },
  "description": "Svelte RSocket Demo",
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack",
    "autobuild": "webpack --watch",
    "start": "webpack-dev-server --open",
    "dev": "run-p start autobuild",
    "rsocket-server": "rsocket-cli -i \"pong\" --server --debug ws://localhost:8080/rsocket",
    "clean": "rm -rf public/build"
  },
  "devDependencies": {
    "cross-env": "^7.0.2",
    "css-loader": "^5.0.1",
    "mini-css-extract-plugin": "^1.3.1",
    "npm-run-all": "^4.1.5",
    "serve": "^11.3.2",
    "style-loader": "^2.0.0",
    "svelte": "^3.29.7",
    "svelte-loader": "^2.13.6",
    "webpack": "^5.6.0",
    "webpack-cli": "^4.2.0",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "fbjs": "^3.0.0",
    "rsocket-core": "^0.0.19",
    "rsocket-flowable": "^0.0.14",
    "rsocket-types": "^0.0.16",
    "rsocket-websocket-client": "^0.0.19",
    "rsocket-websocket-server": "^0.0.19",
    "rxjs": "^6.6.3",
    "svelte-observable": "^0.4.0"
  }
}
